<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

@if(TempData["Exitoso"] != null)
{
    <script>
        Swal.fire({
            position: "top-center",
            icon: "success",
            title: "@TempData["Exitoso"]",
            showConfirmButton: false,
            timer: 3000
        });
    </script>
}

@if (TempData["Error"] != null)
{
    <script>
        Swal.fire({
            position: "top-center",
            icon: "error",
            title: "@TempData["Error"]",
            showConfirmButton: false,
            timer: 3000
        });
    </script>
}

